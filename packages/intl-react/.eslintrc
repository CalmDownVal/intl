{
	"env": {
		"browser": true
	},
	"rules": {
		"import/default": "off",
		"@typescript-eslint/no-unused-vars": [
			"warn",
			{
				"vars": "all",
				"varsIgnorePattern": "(^_|^React$)",
				"args": "all",
				"ignoreRestSiblings": true,
				"argsIgnorePattern": "^_",
				"caughtErrors": "all",
				"caughtErrorsIgnorePattern": "^_"
			}
		]
	}
}
